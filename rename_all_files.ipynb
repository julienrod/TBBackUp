    "import os \n",
    "DIRECTORY = \"output/Test\"\n",
    "PARTICULE = \"c_\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "for filename in os.listdir(DIRECTORY): \n",
    "    if not filename.startswith(PARTICULE):\n",
    "        os.rename(DIRECTORY + \"/\" + filename, DIRECTORY + \"/c_\" + filename) \n"
   ]
